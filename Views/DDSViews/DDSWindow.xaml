<Window x:Class="SiliconRadarControlPanel.Views.DDSViews.DDSWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:SiliconRadarControlPanel.UserControls"
        xmlns:vm="clr-namespace:SiliconRadarControlPanel.ViewModels.DDSViewModels"
        xmlns:ddsviews="clr-namespace:SiliconRadarControlPanel.Views.DDSViews"
        mc:Ignorable="d"
        Title="DDSWindow"
        ResizeMode="NoResize"
        Height="600"
        Width="800">
    <Window.Resources>
        <ResourceDictionary Source="DDSStyles.xaml" />
    </Window.Resources>
    <Window.DataContext>
        <vm:DDSViewModel />
    </Window.DataContext>
    <ScrollViewer >
        <StackPanel>
            <ContentControl Content="{Binding DDSR0ViewModel}"
                            Margin="5,5,5,0" />
            <ddsviews:DDSR1View Margin="5,5,5,0" />
            <ddsviews:DDSR2View Margin="5,5,5,0" />
            <ddsviews:DDSR3View Margin="5,5,5,0" />
            <ddsviews:DDSR4View Margin="5,5,5,0" />
            <ddsviews:DDSR5View Margin="5,5,5,0" />
            <ddsviews:DDSR6View Margin="5,5,5,0" />
            <ddsviews:DDSR7View Margin="5,5,5,0" />
        </StackPanel>
    </ScrollViewer>
</Window>
